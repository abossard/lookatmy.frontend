    "@types/koa-static": "^3.0.1",
    "@types/react": "^16.0.7",
    "@types/react-dom": "^16.0.0",
    "@types/webpack-env": "^1.13.1",
    "adal-ts": "^0.6.0",
    "dotenv": "^4.0.0",
    "koa": "^2.3.0",
    "koa-mount": "^3.0.0",
    "koa-static": "^4.0.1",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.2",
    "mobx-react-devtools": "^4.2.15",
